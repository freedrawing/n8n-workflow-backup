    {
      "id": "3f3486be-fdcd-44a4-95c4-b1a7f9dbf65b",
      "name": "isDiffOrNew",
      "parameters": {
        "jsCode": "// 재귀적으로 비교에서 제외할 키를 제거\nconst removeIgnoredKeys = (obj) => {\n  const ignore = [\n    'createdAt',\n    'updatedAt',\n    'versionId',\n    'meta',\n    'staticData',\n    'pinData'\n  ];\n  if (Array.isArray(obj)) {\n    return obj.map(removeIgnoredKeys);\n  } else if (obj !== null && typeof obj === 'object') {\n    const cleaned = {};\n    for (const [key, value] of Object.entries(obj)) {\n      if (!ignore.includes(key)) {\n        cleaned[key] = removeIgnoredKeys(value);\n      }\n    }\n    return cleaned;\n  }\n  return obj;\n};\n\n// 재귀적으로 키 정렬\nconst deepSortKeys = (obj) => {\n  if (Array.isArray(obj)) {\n    return obj.map(deepSortKeys);\n  } else if (obj !== null && typeof obj === 'object') {\n    return Object.keys(obj).sort().reduce((acc, key) => {\n      acc[key] = deepSortKeys(obj[key]);\n      return acc;\n    }, {});\n  }\n  return obj;\n};\n\n// 비교 전 처리 (필터링 + 정렬)\nconst prepareForCompare = (data) => deepSortKeys(removeIgnoredKeys(data));\n\n// ===== 핵심 로직 시작 =====\n\n// 첫 번째 입력: GitHub에서 읽은 기존 파일\n// 두 번째 입력: n8n에서 가져온 현재 워크플로우\nconst input = $input.all();\n\nconst fileJson = input[0].json;\nconst n8nWorkflow = input[1].json;\n\nlet origWorkflow;\n\n// GitHub에서 가져온 데이터가 base64 content 형태인 경우\nif (Object.keys(fileJson).includes(\"content\")) {\n  origWorkflow = JSON.parse(Buffer.from(fileJson.content, 'base64').toString());\n\n// 혹시 data 키로 JSON이 들어오는 경우\n} else if (Object.keys(fileJson).includes(\"data\")) {\n  origWorkflow = JSON.parse(fileJson.data);\n\n// 새 파일인 경우\n} else {\n  // GitHub에 파일이 없으므로 새로운 상태\n  const orderedActual = prepareForCompare(n8nWorkflow);\n  fileJson.github_status = \"new\";\n  fileJson.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n  return input;\n}\n\n// 두 JSON을 필터링하고 정렬\nconst orderedOriginal = prepareForCompare(origWorkflow);\nconst orderedActual = prepareForCompare(n8nWorkflow);\n\n// JSON 문자열로 비교\nif (JSON.stringify(orderedOriginal) === JSON.stringify(orderedActual)) {\n  fileJson.github_status = \"same\";\n} else {\n  fileJson.github_status = \"different\";\n  fileJson.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n}\n\n// 디코딩된 내용도 보관\nfileJson.content_decoded = orderedOriginal;\n\nreturn input;\n"
      },
      "position": [
        464,
        704
      ],
      "type": "n8n-nodes-base.code",
      "typeVersion": 1
